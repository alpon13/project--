num1=float(input("enter the first number:"))
obaretor=input("enter an opearoter(+,-,*,/)")

num2=float(input("enter the second number:"))

if obaretor=="+":
    ans =num1+num2
    print(f"your result is {ans}")
elif obaretor=="-":
    ans=num1-num2
    print(f"your result is {ans}")
elif obaretor=="/":
    ans=num1/num2
    print(f"your result is {ans}")

elif obaretor == "*":
    ans = num1 * num2
    print(f"your result is  {ans}")
else:
    print("your result is anmatched")
